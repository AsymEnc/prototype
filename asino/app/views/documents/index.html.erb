<h1>Listing documents</h1>

<table class="table">
  <thead>
    <tr>
      <th>Document ID</th>
      <th>Document Title</th>
      <th>Issue Date</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documents.each do |document| %>
      <tr>
        <td><%= document.docid %></td>
        <td><%= document.title %></td>
        <td><%= document.date %></td>
        <td><%= document.location %></td>
        <td><%= link_to 'Show', document %></td>
      </tr>
    <% end %>
  </tbody>
</table>


